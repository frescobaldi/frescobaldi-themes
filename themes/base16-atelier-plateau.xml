<frescobaldi-theme name="#Atelier Plateau">
  <!--
  Created by base16-frescobaldi
  Author: #Bram de Haan (http://atelierbramdehaan.nl)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#8a8585" />
    <background color="#1b1818" />
    <selectiontext color="#e7dfdf" />
    <selectionbackground color="#655d5d" />
    <current color="#292424" />
    <mark color="#585050" />
    <error color="#292424" />
    <search color="#" />
    <match color="#7e7777" />
    <paper color="#292424" />
    <musichighlight color="#655d5d" />
  </baseColors>
  <defaultStyles>
    <string textColor="#ca4949" />
    <escape textColor="#b45a3c" />
    <comment italic="True" textColor="#655d5d" />
    <value textColor="#a06e3b" />
    <variable textColor="#4b8b8b" />
    <keyword bold="True" />
    <function bold="True" textColor="#7272ca" />
    <error textColor="#8464c4" underline="True" underlineColor="#8464c4" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#b45a3c" />
      <dynamic bold="True" textColor="#7272ca" />
      <articulation bold="True" textColor="#b45a3c" />
      <fingering textColor="#4b8b8b" />
      <stringnumber textColor="#b45a3c" />
      <slur bold="True" />
      <markup bold="False" textColor="#a06e3b" />
      <lyricmode textColor="#7272ca" />
      <lyrictext textColor="#8a8585" />
      <context bold="True" />
      <grob textColor="#8a8585" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
