<frescobaldi-theme name="#Atelier Estuary">
  <!--
  Created by base16-frescobaldi
  Author: #Bram de Haan (http://atelierbramdehaan.nl)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#929181" />
    <background color="#22221b" />
    <selectiontext color="#e7e6df" />
    <selectionbackground color="#6c6b5a" />
    <current color="#302f27" />
    <mark color="#5f5e4e" />
    <error color="#302f27" />
    <search color="#" />
    <match color="#878573" />
    <paper color="#302f27" />
    <musichighlight color="#6c6b5a" />
  </baseColors>
  <defaultStyles>
    <string textColor="#ba6236" />
    <escape textColor="#ae7313" />
    <comment italic="True" textColor="#6c6b5a" />
    <value textColor="#a5980d" />
    <variable textColor="#7d9726" />
    <keyword bold="True" />
    <function bold="True" textColor="#36a166" />
    <error textColor="#5f9182" underline="True" underlineColor="#5f9182" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#ae7313" />
      <dynamic bold="True" textColor="#36a166" />
      <articulation bold="True" textColor="#ae7313" />
      <fingering textColor="#7d9726" />
      <stringnumber textColor="#ae7313" />
      <slur bold="True" />
      <markup bold="False" textColor="#a5980d" />
      <lyricmode textColor="#36a166" />
      <lyrictext textColor="#929181" />
      <context bold="True" />
      <grob textColor="#929181" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
