<frescobaldi-theme name="#vulcan">
  <!--
  Created by base16-frescobaldi
  Author: #Andrey Varfolomeev
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#5b778c" />
    <background color="#041523" />
    <selectiontext color="#333238" />
    <selectionbackground color="#7a5759" />
    <current color="#122339" />
    <mark color="#003552" />
    <error color="#122339" />
    <search color="#" />
    <match color="#6b6977" />
    <paper color="#122339" />
    <musichighlight color="#7a5759" />
  </baseColors>
  <defaultStyles>
    <string textColor="#818591" />
    <escape textColor="#9198a3" />
    <comment italic="True" textColor="#7a5759" />
    <value textColor="#adb4b9" />
    <variable textColor="#977d7c" />
    <keyword bold="True" />
    <function bold="True" textColor="#977d7c" />
    <error textColor="#9198a3" underline="True" underlineColor="#9198a3" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#9198a3" />
      <dynamic bold="True" textColor="#977d7c" />
      <articulation bold="True" textColor="#9198a3" />
      <fingering textColor="#977d7c" />
      <stringnumber textColor="#9198a3" />
      <slur bold="True" />
      <markup bold="False" textColor="#adb4b9" />
      <lyricmode textColor="#977d7c" />
      <lyrictext textColor="#5b778c" />
      <context bold="True" />
      <grob textColor="#5b778c" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
