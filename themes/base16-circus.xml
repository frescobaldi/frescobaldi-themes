<frescobaldi-theme name="#Circus">
  <!--
  Created by base16-frescobaldi
  Author: #Stephan Boyer (https://github.com/stepchowfun) and Esther Wang (https://github.com/ewang12)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#a7a7a7" />
    <background color="#191919" />
    <selectiontext color="#808080" />
    <selectionbackground color="#5f5a60" />
    <current color="#202020" />
    <mark color="#303030" />
    <error color="#202020" />
    <search color="#" />
    <match color="#505050" />
    <paper color="#202020" />
    <musichighlight color="#5f5a60" />
  </baseColors>
  <defaultStyles>
    <string textColor="#dc657d" />
    <escape textColor="#4bb1a7" />
    <comment italic="True" textColor="#5f5a60" />
    <value textColor="#c3ba63" />
    <variable textColor="#84b97c" />
    <keyword bold="True" />
    <function bold="True" textColor="#639ee4" />
    <error textColor="#b888e2" underline="True" underlineColor="#b888e2" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#4bb1a7" />
      <dynamic bold="True" textColor="#639ee4" />
      <articulation bold="True" textColor="#4bb1a7" />
      <fingering textColor="#84b97c" />
      <stringnumber textColor="#4bb1a7" />
      <slur bold="True" />
      <markup bold="False" textColor="#c3ba63" />
      <lyricmode textColor="#639ee4" />
      <lyrictext textColor="#a7a7a7" />
      <context bold="True" />
      <grob textColor="#a7a7a7" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
