<frescobaldi-theme name="#Vice Alt">
  <!--
  Created by base16-frescobaldi
  Author: #Thomas Leon Highbaugh
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#d7afff" />
    <background color="#303030" />
    <selectiontext color="#ffffff" />
    <selectionbackground color="#875faf" />
    <current color="#444444" />
    <mark color="#878787" />
    <error color="#444444" />
    <search color="#" />
    <match color="#afafd7" />
    <paper color="#afffd7" />
    <musichighlight color="#875faf" />
  </baseColors>
  <defaultStyles>
    <string textColor="#ff005f" />
    <escape textColor="#ff00ff" />
    <comment italic="True" textColor="#875faf" />
    <value textColor="#ffffaf" />
    <variable textColor="#87ffff" />
    <keyword bold="True" />
    <function bold="True" textColor="#afffff" />
    <error textColor="#d7afff" underline="True" underlineColor="#d7afff" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#ff00ff" />
      <dynamic bold="True" textColor="#afffff" />
      <articulation bold="True" textColor="#ff00ff" />
      <fingering textColor="#87ffff" />
      <stringnumber textColor="#ff00ff" />
      <slur bold="True" />
      <markup bold="False" textColor="#ffffaf" />
      <lyricmode textColor="#afffff" />
      <lyrictext textColor="#d7afff" />
      <context bold="True" />
      <grob textColor="#d7afff" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
