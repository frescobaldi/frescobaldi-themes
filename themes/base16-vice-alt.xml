<frescobaldi-theme name="#Vice Alt">
  <!--
  Created by base16-frescobaldi
  Author: #Thomas Leon Highbaugh
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#555555" />
    <background color="#1c1c1c" />
    <selectiontext color="#b6b6b6" />
    <selectionbackground color="#323232" />
    <current color="#282828" />
    <mark color="#2c2c2c" />
    <error color="#282828" />
    <search color="#" />
    <match color="#3c3c3c" />
    <paper color="#282828" />
    <musichighlight color="#323232" />
  </baseColors>
  <defaultStyles>
    <string textColor="#ff3d81" />
    <escape textColor="#F67544" />
    <comment italic="True" textColor="#323232" />
    <value textColor="#ffff73" />
    <variable textColor="#44ffdd" />
    <keyword bold="True" />
    <function bold="True" textColor="#2fb1d4" />
    <error textColor="#8265ff" underline="True" underlineColor="#8265ff" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#F67544" />
      <dynamic bold="True" textColor="#2fb1d4" />
      <articulation bold="True" textColor="#F67544" />
      <fingering textColor="#44ffdd" />
      <stringnumber textColor="#F67544" />
      <slur bold="True" />
      <markup bold="False" textColor="#ffff73" />
      <lyricmode textColor="#2fb1d4" />
      <lyrictext textColor="#555555" />
      <context bold="True" />
      <grob textColor="#555555" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
