<frescobaldi-theme name="#Railscasts">
  <!--
  Created by base16-frescobaldi
  Author: #Ryan Bates (http://railscasts.com)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#e6e1dc" />
    <background color="#2b2b2b" />
    <selectiontext color="#f4f1ed" />
    <selectionbackground color="#5a647e" />
    <current color="#272935" />
    <mark color="#3a4055" />
    <error color="#272935" />
    <search color="#" />
    <match color="#d4cfc9" />
    <paper color="#272935" />
    <musichighlight color="#5a647e" />
  </baseColors>
  <defaultStyles>
    <string textColor="#da4939" />
    <escape textColor="#cc7833" />
    <comment italic="True" textColor="#5a647e" />
    <value textColor="#ffc66d" />
    <variable textColor="#a5c261" />
    <keyword bold="True" />
    <function bold="True" textColor="#6d9cbe" />
    <error textColor="#b6b3eb" underline="True" underlineColor="#b6b3eb" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#cc7833" />
      <dynamic bold="True" textColor="#6d9cbe" />
      <articulation bold="True" textColor="#cc7833" />
      <fingering textColor="#a5c261" />
      <stringnumber textColor="#cc7833" />
      <slur bold="True" />
      <markup bold="False" textColor="#ffc66d" />
      <lyricmode textColor="#6d9cbe" />
      <lyrictext textColor="#e6e1dc" />
      <context bold="True" />
      <grob textColor="#e6e1dc" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
