<frescobaldi-theme name="#Equilibrium Light">
  <!--
  Created by base16-frescobaldi
  Author: #Carlo Abelli
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#43474e" />
    <background color="#f5f0e7" />
    <selectiontext color="#2c3138" />
    <selectionbackground color="#73777f" />
    <current color="#e7e2d9" />
    <mark color="#d8d4cb" />
    <error color="#e7e2d9" />
    <search color="#" />
    <match color="#5a5f66" />
    <paper color="#e7e2d9" />
    <musichighlight color="#73777f" />
  </baseColors>
  <defaultStyles>
    <string textColor="#d02023" />
    <escape textColor="#bf3e05" />
    <comment italic="True" textColor="#73777f" />
    <value textColor="#9d6f00" />
    <variable textColor="#637200" />
    <keyword bold="True" />
    <function bold="True" textColor="#0073b5" />
    <error textColor="#4e66b6" underline="True" underlineColor="#4e66b6" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#bf3e05" />
      <dynamic bold="True" textColor="#0073b5" />
      <articulation bold="True" textColor="#bf3e05" />
      <fingering textColor="#637200" />
      <stringnumber textColor="#bf3e05" />
      <slur bold="True" />
      <markup bold="False" textColor="#9d6f00" />
      <lyricmode textColor="#0073b5" />
      <lyrictext textColor="#43474e" />
      <context bold="True" />
      <grob textColor="#43474e" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
