<frescobaldi-theme name="#Mexico Light">
  <!--
  Created by base16-frescobaldi
  Author: #Sheldon Johnson
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#383838" />
    <background color="#f8f8f8" />
    <selectiontext color="#282828" />
    <selectionbackground color="#b8b8b8" />
    <current color="#e8e8e8" />
    <mark color="#d8d8d8" />
    <error color="#e8e8e8" />
    <search color="#" />
    <match color="#585858" />
    <paper color="#e8e8e8" />
    <musichighlight color="#b8b8b8" />
  </baseColors>
  <defaultStyles>
    <string textColor="#ab4642" />
    <escape textColor="#dc9656" />
    <comment italic="True" textColor="#b8b8b8" />
    <value textColor="#f79a0e" />
    <variable textColor="#538947" />
    <keyword bold="True" />
    <function bold="True" textColor="#7cafc2" />
    <error textColor="#96609e" underline="True" underlineColor="#96609e" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#dc9656" />
      <dynamic bold="True" textColor="#7cafc2" />
      <articulation bold="True" textColor="#dc9656" />
      <fingering textColor="#538947" />
      <stringnumber textColor="#dc9656" />
      <slur bold="True" />
      <markup bold="False" textColor="#f79a0e" />
      <lyricmode textColor="#7cafc2" />
      <lyrictext textColor="#383838" />
      <context bold="True" />
      <grob textColor="#383838" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
