<frescobaldi-theme name="#Atelier Forest">
  <!--
  Created by base16-frescobaldi
  Author: #Bram de Haan (http://atelierbramdehaan.nl)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#a8a19f" />
    <background color="#1b1918" />
    <selectiontext color="#e6e2e0" />
    <selectionbackground color="#766e6b" />
    <current color="#2c2421" />
    <mark color="#68615e" />
    <error color="#2c2421" />
    <search color="#" />
    <match color="#9c9491" />
    <paper color="#2c2421" />
    <musichighlight color="#766e6b" />
  </baseColors>
  <defaultStyles>
    <string textColor="#f22c40" />
    <escape textColor="#df5320" />
    <comment italic="True" textColor="#766e6b" />
    <value textColor="#c38418" />
    <variable textColor="#7b9726" />
    <keyword bold="True" />
    <function bold="True" textColor="#407ee7" />
    <error textColor="#6666ea" underline="True" underlineColor="#6666ea" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#df5320" />
      <dynamic bold="True" textColor="#407ee7" />
      <articulation bold="True" textColor="#df5320" />
      <fingering textColor="#7b9726" />
      <stringnumber textColor="#df5320" />
      <slur bold="True" />
      <markup bold="False" textColor="#c38418" />
      <lyricmode textColor="#407ee7" />
      <lyrictext textColor="#a8a19f" />
      <context bold="True" />
      <grob textColor="#a8a19f" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
