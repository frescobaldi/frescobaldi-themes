<frescobaldi-theme name="#Default Dark">
  <!--
  Created by base16-frescobaldi
  Author: #Chris Kempson (http://chriskempson.com)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#d8d8d8" />
    <background color="#181818" />
    <selectiontext color="#e8e8e8" />
    <selectionbackground color="#585858" />
    <current color="#282828" />
    <mark color="#383838" />
    <error color="#282828" />
    <search color="#" />
    <match color="#b8b8b8" />
    <paper color="#282828" />
    <musichighlight color="#585858" />
  </baseColors>
  <defaultStyles>
    <string textColor="#ab4642" />
    <escape textColor="#dc9656" />
    <comment italic="True" textColor="#585858" />
    <value textColor="#f7ca88" />
    <variable textColor="#a1b56c" />
    <keyword bold="True" />
    <function bold="True" textColor="#7cafc2" />
    <error textColor="#ba8baf" underline="True" underlineColor="#ba8baf" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#dc9656" />
      <dynamic bold="True" textColor="#7cafc2" />
      <articulation bold="True" textColor="#dc9656" />
      <fingering textColor="#a1b56c" />
      <stringnumber textColor="#dc9656" />
      <slur bold="True" />
      <markup bold="False" textColor="#f7ca88" />
      <lyricmode textColor="#7cafc2" />
      <lyrictext textColor="#d8d8d8" />
      <context bold="True" />
      <grob textColor="#d8d8d8" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
