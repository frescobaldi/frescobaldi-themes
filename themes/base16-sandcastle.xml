<frescobaldi-theme name="#Sandcastle">
  <!--
  Created by base16-frescobaldi
  Author: #George Essig (https://github.com/gessig)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#a89984" />
    <background color="#282c34" />
    <selectiontext color="#d5c4a1" />
    <selectionbackground color="#665c54" />
    <current color="#2c323b" />
    <mark color="#3e4451" />
    <error color="#2c323b" />
    <search color="#" />
    <match color="#928374" />
    <paper color="#2c323b" />
    <musichighlight color="#665c54" />
  </baseColors>
  <defaultStyles>
    <string textColor="#83a598" />
    <escape textColor="#a07e3b" />
    <comment italic="True" textColor="#665c54" />
    <value textColor="#a07e3b" />
    <variable textColor="#528b8b" />
    <keyword bold="True" />
    <function bold="True" textColor="#83a598" />
    <error textColor="#d75f5f" underline="True" underlineColor="#d75f5f" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#a07e3b" />
      <dynamic bold="True" textColor="#83a598" />
      <articulation bold="True" textColor="#a07e3b" />
      <fingering textColor="#528b8b" />
      <stringnumber textColor="#a07e3b" />
      <slur bold="True" />
      <markup bold="False" textColor="#a07e3b" />
      <lyricmode textColor="#83a598" />
      <lyrictext textColor="#a89984" />
      <context bold="True" />
      <grob textColor="#a89984" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
