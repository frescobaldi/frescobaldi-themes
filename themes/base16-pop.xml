<frescobaldi-theme name="#Pop">
  <!--
  Created by base16-frescobaldi
  Author: #Chris Kempson (http://chriskempson.com)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#d0d0d0" />
    <background color="#000000" />
    <selectiontext color="#e0e0e0" />
    <selectionbackground color="#505050" />
    <current color="#202020" />
    <mark color="#303030" />
    <error color="#202020" />
    <search color="#" />
    <match color="#b0b0b0" />
    <paper color="#202020" />
    <musichighlight color="#505050" />
  </baseColors>
  <defaultStyles>
    <string textColor="#eb008a" />
    <escape textColor="#f29333" />
    <comment italic="True" textColor="#505050" />
    <value textColor="#f8ca12" />
    <variable textColor="#37b349" />
    <keyword bold="True" />
    <function bold="True" textColor="#0e5a94" />
    <error textColor="#b31e8d" underline="True" underlineColor="#b31e8d" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#f29333" />
      <dynamic bold="True" textColor="#0e5a94" />
      <articulation bold="True" textColor="#f29333" />
      <fingering textColor="#37b349" />
      <stringnumber textColor="#f29333" />
      <slur bold="True" />
      <markup bold="False" textColor="#f8ca12" />
      <lyricmode textColor="#0e5a94" />
      <lyrictext textColor="#d0d0d0" />
      <context bold="True" />
      <grob textColor="#d0d0d0" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
