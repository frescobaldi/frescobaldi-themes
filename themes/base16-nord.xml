<frescobaldi-theme name="#Nord">
  <!--
  Created by base16-frescobaldi
  Author: #arcticicestudio
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#E5E9F0" />
    <background color="#2E3440" />
    <selectiontext color="#ECEFF4" />
    <selectionbackground color="#4C566A" />
    <current color="#3B4252" />
    <mark color="#434C5E" />
    <error color="#3B4252" />
    <search color="#" />
    <match color="#D8DEE9" />
    <paper color="#8FBCBB" />
    <musichighlight color="#4C566A" />
  </baseColors>
  <defaultStyles>
    <string textColor="#88C0D0" />
    <escape textColor="#81A1C1" />
    <comment italic="True" textColor="#4C566A" />
    <value textColor="#5E81AC" />
    <variable textColor="#BF616A" />
    <keyword bold="True" />
    <function bold="True" textColor="#EBCB8B" />
    <error textColor="#A3BE8C" underline="True" underlineColor="#A3BE8C" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#81A1C1" />
      <dynamic bold="True" textColor="#EBCB8B" />
      <articulation bold="True" textColor="#81A1C1" />
      <fingering textColor="#BF616A" />
      <stringnumber textColor="#81A1C1" />
      <slur bold="True" />
      <markup bold="False" textColor="#5E81AC" />
      <lyricmode textColor="#EBCB8B" />
      <lyrictext textColor="#E5E9F0" />
      <context bold="True" />
      <grob textColor="#E5E9F0" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
