<frescobaldi-theme name="#Synth Midnight Terminal Light">
  <!--
  Created by base16-frescobaldi
  Author: #MichaÃ«l Ball (http://github.com/michael-ball/)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#28292a" />
    <background color="#dddfe0" />
    <selectiontext color="#1a1b1c" />
    <selectionbackground color="#a3a5a6" />
    <current color="#cfd1d2" />
    <mark color="#c1c3c4" />
    <error color="#cfd1d2" />
    <search color="#" />
    <match color="#474849" />
    <paper color="#cfd1d2" />
    <musichighlight color="#a3a5a6" />
  </baseColors>
  <defaultStyles>
    <string textColor="#b53b50" />
    <escape textColor="#ea770d" />
    <comment italic="True" textColor="#a3a5a6" />
    <value textColor="#c9d364" />
    <variable textColor="#06ea61" />
    <keyword bold="True" />
    <function bold="True" textColor="#03aeff" />
    <error textColor="#ea5ce2" underline="True" underlineColor="#ea5ce2" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#ea770d" />
      <dynamic bold="True" textColor="#03aeff" />
      <articulation bold="True" textColor="#ea770d" />
      <fingering textColor="#06ea61" />
      <stringnumber textColor="#ea770d" />
      <slur bold="True" />
      <markup bold="False" textColor="#c9d364" />
      <lyricmode textColor="#03aeff" />
      <lyrictext textColor="#28292a" />
      <context bold="True" />
      <grob textColor="#28292a" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
