<frescobaldi-theme name="#Grayscale Dark">
  <!--
  Created by base16-frescobaldi
  Author: #Alexandre Gavioli (https://github.com/Alexx2/)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#b9b9b9" />
    <background color="#101010" />
    <selectiontext color="#e3e3e3" />
    <selectionbackground color="#525252" />
    <current color="#252525" />
    <mark color="#464646" />
    <error color="#252525" />
    <search color="#" />
    <match color="#ababab" />
    <paper color="#252525" />
    <musichighlight color="#525252" />
  </baseColors>
  <defaultStyles>
    <string textColor="#7c7c7c" />
    <escape textColor="#999999" />
    <comment italic="True" textColor="#525252" />
    <value textColor="#a0a0a0" />
    <variable textColor="#8e8e8e" />
    <keyword bold="True" />
    <function bold="True" textColor="#686868" />
    <error textColor="#747474" underline="True" underlineColor="#747474" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#999999" />
      <dynamic bold="True" textColor="#686868" />
      <articulation bold="True" textColor="#999999" />
      <fingering textColor="#8e8e8e" />
      <stringnumber textColor="#999999" />
      <slur bold="True" />
      <markup bold="False" textColor="#a0a0a0" />
      <lyricmode textColor="#686868" />
      <lyrictext textColor="#b9b9b9" />
      <context bold="True" />
      <grob textColor="#b9b9b9" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
