<frescobaldi-theme name="#Espresso">
  <!--
  Created by base16-frescobaldi
  Author: #Unknown. Maintained by Alex Mirrington (https://github.com/alexmirrington)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#cccccc" />
    <background color="#2d2d2d" />
    <selectiontext color="#e0e0e0" />
    <selectionbackground color="#777777" />
    <current color="#393939" />
    <mark color="#515151" />
    <error color="#393939" />
    <search color="#" />
    <match color="#b4b7b4" />
    <paper color="#393939" />
    <musichighlight color="#777777" />
  </baseColors>
  <defaultStyles>
    <string textColor="#d25252" />
    <escape textColor="#f9a959" />
    <comment italic="True" textColor="#777777" />
    <value textColor="#ffc66d" />
    <variable textColor="#a5c261" />
    <keyword bold="True" />
    <function bold="True" textColor="#6c99bb" />
    <error textColor="#d197d9" underline="True" underlineColor="#d197d9" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#f9a959" />
      <dynamic bold="True" textColor="#6c99bb" />
      <articulation bold="True" textColor="#f9a959" />
      <fingering textColor="#a5c261" />
      <stringnumber textColor="#f9a959" />
      <slur bold="True" />
      <markup bold="False" textColor="#ffc66d" />
      <lyricmode textColor="#6c99bb" />
      <lyrictext textColor="#cccccc" />
      <context bold="True" />
      <grob textColor="#cccccc" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
