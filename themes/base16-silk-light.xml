<frescobaldi-theme name="#Silk Light">
  <!--
  Created by base16-frescobaldi
  Author: #Gabriel Fontes (https://github.com/Misterio77)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#385156" />
    <background color="#E9F1EF" />
    <selectiontext color="#0e3c46" />
    <selectionbackground color="#5C787B" />
    <current color="#CCD4D3" />
    <mark color="#90B7B6" />
    <error color="#CCD4D3" />
    <search color="#" />
    <match color="#4B5B5F" />
    <paper color="#CCD4D3" />
    <musichighlight color="#5C787B" />
  </baseColors>
  <defaultStyles>
    <string textColor="#CF432E" />
    <escape textColor="#D27F46" />
    <comment italic="True" textColor="#5C787B" />
    <value textColor="#CFAD25" />
    <variable textColor="#6CA38C" />
    <keyword bold="True" />
    <function bold="True" textColor="#39AAC9" />
    <error textColor="#6E6582" underline="True" underlineColor="#6E6582" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#D27F46" />
      <dynamic bold="True" textColor="#39AAC9" />
      <articulation bold="True" textColor="#D27F46" />
      <fingering textColor="#6CA38C" />
      <stringnumber textColor="#D27F46" />
      <slur bold="True" />
      <markup bold="False" textColor="#CFAD25" />
      <lyricmode textColor="#39AAC9" />
      <lyrictext textColor="#385156" />
      <context bold="True" />
      <grob textColor="#385156" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
