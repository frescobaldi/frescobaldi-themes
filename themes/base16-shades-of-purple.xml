<frescobaldi-theme name="#Shades of Purple">
  <!--
  Created by base16-frescobaldi
  Author: #Iolar Demartini Junior (http://github.com/demartini) based on Shades of Purple Theme (https://github.com/ahmadawais/shades-of-purple-vscode).
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#c7c7c7" />
    <background color="#1e1e3f" />
    <selectiontext color="#ff77ff" />
    <selectionbackground color="#808080" />
    <current color="#43d426" />
    <mark color="#f1d000" />
    <error color="#43d426" />
    <search color="#" />
    <match color="#6871ff" />
    <paper color="#43d426" />
    <musichighlight color="#808080" />
  </baseColors>
  <defaultStyles>
    <string textColor="#d90429" />
    <escape textColor="#f92a1c" />
    <comment italic="True" textColor="#808080" />
    <value textColor="#ffe700" />
    <variable textColor="#3ad900" />
    <keyword bold="True" />
    <function bold="True" textColor="#6943ff" />
    <error textColor="#ff2c70" underline="True" underlineColor="#ff2c70" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#f92a1c" />
      <dynamic bold="True" textColor="#6943ff" />
      <articulation bold="True" textColor="#f92a1c" />
      <fingering textColor="#3ad900" />
      <stringnumber textColor="#f92a1c" />
      <slur bold="True" />
      <markup bold="False" textColor="#ffe700" />
      <lyricmode textColor="#6943ff" />
      <lyrictext textColor="#c7c7c7" />
      <context bold="True" />
      <grob textColor="#c7c7c7" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
