<frescobaldi-theme name="#Atelier Sulphurpool Light">
  <!--
  Created by base16-frescobaldi
  Author: #Bram de Haan (http://atelierbramdehaan.nl)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#5e6687" />
    <background color="#f5f7ff" />
    <selectiontext color="#293256" />
    <selectionbackground color="#898ea4" />
    <current color="#dfe2f1" />
    <mark color="#979db4" />
    <error color="#dfe2f1" />
    <search color="#" />
    <match color="#6b7394" />
    <paper color="#dfe2f1" />
    <musichighlight color="#898ea4" />
  </baseColors>
  <defaultStyles>
    <string textColor="#c94922" />
    <escape textColor="#c76b29" />
    <comment italic="True" textColor="#898ea4" />
    <value textColor="#c08b30" />
    <variable textColor="#ac9739" />
    <keyword bold="True" />
    <function bold="True" textColor="#3d8fd1" />
    <error textColor="#6679cc" underline="True" underlineColor="#6679cc" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#c76b29" />
      <dynamic bold="True" textColor="#3d8fd1" />
      <articulation bold="True" textColor="#c76b29" />
      <fingering textColor="#ac9739" />
      <stringnumber textColor="#c76b29" />
      <slur bold="True" />
      <markup bold="False" textColor="#c08b30" />
      <lyricmode textColor="#3d8fd1" />
      <lyrictext textColor="#5e6687" />
      <context bold="True" />
      <grob textColor="#5e6687" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
