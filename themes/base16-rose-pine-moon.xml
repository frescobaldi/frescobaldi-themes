<frescobaldi-theme name="#RosÃ© Pine Moon">
  <!--
  Created by base16-frescobaldi
  Author: #Emilia Dunfelt &lt;sayhi@dunfelt.se&gt;
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#e0def4" />
    <background color="#232136" />
    <selectiontext color="#f5f5f7" />
    <selectionbackground color="#59546d" />
    <current color="#2a273f" />
    <mark color="#393552" />
    <error color="#2a273f" />
    <search color="#" />
    <match color="#817c9c" />
    <paper color="#2a273f" />
    <musichighlight color="#59546d" />
  </baseColors>
  <defaultStyles>
    <string textColor="#ecebf0" />
    <escape textColor="#eb6f92" />
    <comment italic="True" textColor="#59546d" />
    <value textColor="#f6c177" />
    <variable textColor="#ea9a97" />
    <keyword bold="True" />
    <function bold="True" textColor="#9ccfd8" />
    <error textColor="#c4a7e7" underline="True" underlineColor="#c4a7e7" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#eb6f92" />
      <dynamic bold="True" textColor="#9ccfd8" />
      <articulation bold="True" textColor="#eb6f92" />
      <fingering textColor="#ea9a97" />
      <stringnumber textColor="#eb6f92" />
      <slur bold="True" />
      <markup bold="False" textColor="#f6c177" />
      <lyricmode textColor="#9ccfd8" />
      <lyrictext textColor="#e0def4" />
      <context bold="True" />
      <grob textColor="#e0def4" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
