<frescobaldi-theme name="#Ocean">
  <!--
  Created by base16-frescobaldi
  Author: #Chris Kempson (http://chriskempson.com)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#c0c5ce" />
    <background color="#2b303b" />
    <selectiontext color="#dfe1e8" />
    <selectionbackground color="#65737e" />
    <current color="#343d46" />
    <mark color="#4f5b66" />
    <error color="#343d46" />
    <search color="#" />
    <match color="#a7adba" />
    <paper color="#343d46" />
    <musichighlight color="#65737e" />
  </baseColors>
  <defaultStyles>
    <string textColor="#bf616a" />
    <escape textColor="#d08770" />
    <comment italic="True" textColor="#65737e" />
    <value textColor="#ebcb8b" />
    <variable textColor="#a3be8c" />
    <keyword bold="True" />
    <function bold="True" textColor="#8fa1b3" />
    <error textColor="#b48ead" underline="True" underlineColor="#b48ead" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#d08770" />
      <dynamic bold="True" textColor="#8fa1b3" />
      <articulation bold="True" textColor="#d08770" />
      <fingering textColor="#a3be8c" />
      <stringnumber textColor="#d08770" />
      <slur bold="True" />
      <markup bold="False" textColor="#ebcb8b" />
      <lyricmode textColor="#8fa1b3" />
      <lyrictext textColor="#c0c5ce" />
      <context bold="True" />
      <grob textColor="#c0c5ce" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
