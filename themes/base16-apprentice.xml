<frescobaldi-theme name="#Apprentice">
  <!--
  Created by base16-frescobaldi
  Author: #romainl
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#5F5F87" />
    <background color="#262626" />
    <selectiontext color="#5F8787" />
    <selectionbackground color="#87875F" />
    <current color="#AF5F5F" />
    <mark color="#5F875F" />
    <error color="#AF5F5F" />
    <search color="#" />
    <match color="#5F87AF" />
    <paper color="#AF5F5F" />
    <musichighlight color="#87875F" />
  </baseColors>
  <defaultStyles>
    <string textColor="#444444" />
    <escape textColor="#FF8700" />
    <comment italic="True" textColor="#87875F" />
    <value textColor="#87AF87" />
    <variable textColor="#FFFFAF" />
    <keyword bold="True" />
    <function bold="True" textColor="#8787AF" />
    <error textColor="#5FAFAF" underline="True" underlineColor="#5FAFAF" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#FF8700" />
      <dynamic bold="True" textColor="#8787AF" />
      <articulation bold="True" textColor="#FF8700" />
      <fingering textColor="#FFFFAF" />
      <stringnumber textColor="#FF8700" />
      <slur bold="True" />
      <markup bold="False" textColor="#87AF87" />
      <lyricmode textColor="#8787AF" />
      <lyrictext textColor="#5F5F87" />
      <context bold="True" />
      <grob textColor="#5F5F87" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
