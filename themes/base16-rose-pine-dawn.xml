<frescobaldi-theme name="#RosÃ© Pine Dawn">
  <!--
  Created by base16-frescobaldi
  Author: #Emilia Dunfelt &lt;edun@dunfelt.se&gt;
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#575279" />
    <background color="#faf4ed" />
    <selectiontext color="#575279" />
    <selectionbackground color="#9893a5" />
    <current color="#fffaf3" />
    <mark color="#f2e9de" />
    <error color="#fffaf3" />
    <search color="#" />
    <match color="#797593" />
    <paper color="#fffaf3" />
    <musichighlight color="#9893a5" />
  </baseColors>
  <defaultStyles>
    <string textColor="#b4637a" />
    <escape textColor="#ea9d34" />
    <comment italic="True" textColor="#9893a5" />
    <value textColor="#d7827e" />
    <variable textColor="#286983" />
    <keyword bold="True" />
    <function bold="True" textColor="#907aa9" />
    <error textColor="#ea9d34" underline="True" underlineColor="#ea9d34" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#ea9d34" />
      <dynamic bold="True" textColor="#907aa9" />
      <articulation bold="True" textColor="#ea9d34" />
      <fingering textColor="#286983" />
      <stringnumber textColor="#ea9d34" />
      <slur bold="True" />
      <markup bold="False" textColor="#d7827e" />
      <lyricmode textColor="#907aa9" />
      <lyrictext textColor="#575279" />
      <context bold="True" />
      <grob textColor="#575279" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
