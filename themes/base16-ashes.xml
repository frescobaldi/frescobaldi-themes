<frescobaldi-theme name="#Ashes">
  <!--
  Created by base16-frescobaldi
  Author: #Jannik Siebert (https://github.com/janniks)
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#C7CCD1" />
    <background color="#1C2023" />
    <selectiontext color="#DFE2E5" />
    <selectionbackground color="#747C84" />
    <current color="#393F45" />
    <mark color="#565E65" />
    <error color="#393F45" />
    <search color="#" />
    <match color="#ADB3BA" />
    <paper color="#393F45" />
    <musichighlight color="#747C84" />
  </baseColors>
  <defaultStyles>
    <string textColor="#C7AE95" />
    <escape textColor="#C7C795" />
    <comment italic="True" textColor="#747C84" />
    <value textColor="#AEC795" />
    <variable textColor="#95C7AE" />
    <keyword bold="True" />
    <function bold="True" textColor="#AE95C7" />
    <error textColor="#C795AE" underline="True" underlineColor="#C795AE" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#C7C795" />
      <dynamic bold="True" textColor="#AE95C7" />
      <articulation bold="True" textColor="#C7C795" />
      <fingering textColor="#95C7AE" />
      <stringnumber textColor="#C7C795" />
      <slur bold="True" />
      <markup bold="False" textColor="#AEC795" />
      <lyricmode textColor="#AE95C7" />
      <lyrictext textColor="#C7CCD1" />
      <context bold="True" />
      <grob textColor="#C7CCD1" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
