<frescobaldi-theme name="#Hardcore">
  <!--
  Created by base16-frescobaldi
  Author: #Chris Caller
-->
  <font fontFamily="monospace" fontSize="10.0" />
  <baseColors>
    <text color="#cdcdcd" />
    <background color="#212121" />
    <selectiontext color="#e5e5e5" />
    <selectionbackground color="#4A4A4A" />
    <current color="#303030" />
    <mark color="#353535" />
    <error color="#303030" />
    <search color="#" />
    <match color="#707070" />
    <paper color="#303030" />
    <musichighlight color="#4A4A4A" />
  </baseColors>
  <defaultStyles>
    <string textColor="#f92672" />
    <escape textColor="#fd971f" />
    <comment italic="True" textColor="#4A4A4A" />
    <value textColor="#e6db74" />
    <variable textColor="#a6e22e" />
    <keyword bold="True" />
    <function bold="True" textColor="#66d9ef" />
    <error textColor="#9e6ffe" underline="True" underlineColor="#9e6ffe" />
  </defaultStyles>
  <allStyles>
    <lilypond>
      <duration textColor="#fd971f" />
      <dynamic bold="True" textColor="#66d9ef" />
      <articulation bold="True" textColor="#fd971f" />
      <fingering textColor="#a6e22e" />
      <stringnumber textColor="#fd971f" />
      <slur bold="True" />
      <markup bold="False" textColor="#e6db74" />
      <lyricmode textColor="#66d9ef" />
      <lyrictext textColor="#cdcdcd" />
      <context bold="True" />
      <grob textColor="#cdcdcd" />
    </lilypond>
  </allStyles>
</frescobaldi-theme>
